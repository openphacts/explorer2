<div class="search-exact-match big-margin-top">
{{#if result.isCompound}}
    <div class="big-margin-right thumbnail left">{{cs_image_src result.csUri}}</div>
    <h1>{{#linkTo compound result}}{{result.prefLabel}}{{/linkTo}}</h1>
    <div class="clear"></div>
    <h3 class="result-smiles big-margin-right big-margin-top">{{result.smiles}}</h3>
    <dl class="big-margin-top">
    <dt class="medium-padding-bottom"><h3>Description</h3></dt>
    <dd>{{result.description}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Toxicity</h3></dt>
    <dd>{{result.toxicity}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Protein Binding</h3></dt>
    <dd>{{result.proteinBinding}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Bio-transformation</h3></dt>
    <dd>{{result.biotransformationItem}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Molecular Weight</h3></dt>
    <dd>{{result.fullMWT}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3># H-Bond Acceptors</h3></dt>
    <dd>{{result.hba}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3># H-Bond Donors</h3></dt>
    <dd>{{result.hbd}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Inchi</h3></dt>
    <dd>{{result.inchi}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>MW Freebase</h3></dt>
    <dd>{{result.mwFreebase}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>ro5 violations</h3></dt>
    <dd>{{result.ro5Violations}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Rotatable Bonds</h3></dt>
    <dd>{{result.rtb}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>AlogP</h3></dt>
    <dd>{{result.logp}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Molecular form</h3></dt>
    <dd>{{result.molform}}</dd>
    <dt class="medium-padding-bottom medium-padding-top"><h3>Polar Surface Area</h3></dt>
    <dd>{{result.psa}}</dd>
    </dl>
{{/if}}
</div>
