<!-- breadcrumb	-->
<ul class="breadcrumb">
	<li><a href="/">Home</a></li>
	<li><span class="divider">/</span></li>
	<li class="active">Favourites</li>
</ul>

<div class="container-fluid">
<ul class="nav nav-tabs">
	  <li class="active"><a href="#favourite-compounds" data-toggle="tab">Compounds</a></li>
	    <li><a href="#favourite-targets" data-toggle="tab">Targets</a></li>
    </ul>

    <div class="tab-content">
	    <div class="tab-pane active" id="favourite-compounds">
	    <table class="mytable">
	<thead  class="headingstyle">
		<tr><th>Image</th><th>Label</th></tr>
	</thead>
	<tbody>
{{#each}}
<tr class="record-deco">
	{{#if isCompound}}
	<td>{{cs_image_src csURI}}</td><td>{{#link-to "compounds.index" (query-params uri=URI)}}{{prefLabel}}{{/link-to}}</td>
	{{/if}}
</tr>
{{/each}}
</tbody>
</table>

	    
	    </div>
	    <div class="tab-pane" id="favourite-targets">
	    <table class="mytable">
	<thead  class="headingstyle">
		<tr><th>Image</th><th>Label</th></tr>
	</thead>
	<tbody>
{{#each}}
<tr class="record-deco">
	{{#if isTarget}}
	<td>{{target_image_src seeAlso}}</td><td>{{#link-to "targets.index" (query-params uri=URI)}}{{prefLabel}}{{/link-to}}</td>
	{{/if}}
</tr>
{{/each}}
</tbody>
</table>

	    </div>
	    </div>
</div>
