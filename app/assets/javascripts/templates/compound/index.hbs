<style>
	dt { 
		padding: 20px 0 0 0;
	}
	dd { 
		padding: 20px 0 0 0; }
</style>
<div>

	<!-- breadcrumb	-->
	<ul class="breadcrumb">
  		<li><a href="/">Home</a></li>
		<li><span class="divider">/</span>{{prefLabel}}<span class="divider">/</span></li>
  		<li class="active">Compound Information</li>

		<div class="row pull-right"> 
			<div class="span1 small-padding-top small-padding-right">Provenance</div>
  			<div class="span2 btn-group" data-toggle="buttons-radio">
  				<button class="btn">On</button>
  				<button class="btn active">Off</button>
  			</div>
		</div>
	</ul>
	
	
    			<div> &larr; {{#linkTo 'search'}} back to search{{/linkTo}}</div>
  		
	
	<!-- main -->
	<div class="container">
	<div class="container-fluid large-top-spacing">
  		<div class="row-fluid">
  			
  			<!--Sidebar content-->
    		<div class="span3" style="min-height:800px;">
      			<div style="padding:0 0 0 20px;" >{{cs_image_src csUri}}</div>
      			
      			<!--box properties-->
      			<div id="boxes" class="small-top-spacing">
      			
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small>AlogP</small></p>
  							<p class="lead">{{logp}}</p>
  						</div>
      				</div>
      				
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small># H-Bond Acceptors</small></p>
  							<p class="lead">{{hba}}</p>
  						</div>
      				</div>
      				
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small># H-Bond Donors</small></p>
  							<p class="lead">{{hbd}}</p>
  						</div>
      				</div>
      				
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small>Mol Weight</small></p>
  							<p class="lead">{{fullMWT}}</p>
  						</div>
      				</div>
      				
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small>Mol Weight Freebase</small></p>
  							<p class="lead">{{mwFreebase}}</p>
  						</div>
      				</div>
      				
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small>Polar Surface Area</small></p>
  							<p class="lead">{{psa}}</p>
  						</div>
      				</div>
      				<div class="summary-box">
      					<div class="inner-property-content">
  							<p><small># Rotatable Bonds</small></p>
  							<p class="lead">{{rtb}}</p>
  						</div>
      				</div>
      				
      			</div>
    		</div>
    		
    		<!--Body content-->
    		<div class="span9" style="padding:30px 0 0 0;">
     			
				<!--title and formula-->
				<div>
					<div class="span3 lead pull-right medium-top-spacing">
						<p class="pull-right">{{formatMolecularFormula molform}}</p>
					</div>	
					<div class="span7">
						<h1>{{prefLabel}}</h1>
					</div>
					
					
				</div>
				
				<div style="padding:80px 0 0 0;text-align:left;">
					{{#linkTo 'compound.pharmacology'}}Pharmacology Data{{/linkTo}}
					</div>
							
				<!--description-->
				<div class="summary-descriptive-text large-top-spacing">
					<p>{{description}}</p>
				</div>
				
				<!--biotransformation-->
				<div class="summary-descriptive-text small-top-spacing">
					<p>{{biotransformationItem}}</p>
				</div>
				
				<!--details-->
				<div class="dl-horizontal" style="font-size:13px;">
					<dl>
  						<dt>ChemSpider ID</dt>
  						<dd>{{linkableChemspiderID csUri}}</dd>
  						<dt>SMILES</dt>
  						<dd>{{smiles}}</dd>
  						<dt>Standard InChi</dt>
  						<dd>{{inchi}}</dd>
  						<dt>Standard InChiKey</dt>
  						<dd>{{inchiKey}}</dd>
  						<dt>Protien Binding</dt>
  						<dd>{{proteinBinding}}</dd>
  						<dt>Toxicity</dt>
  						<dd>{{toxicity}}</dd>
					</dl>
     			</div>
     			
     			
    		</div>
  		</div>
	</div>
	</div>
	

	
	
</div>

<script type="text/javascript">
  $(window).off("scroll", scrollHandler);
</script>
