<div {{bind-attr class="fetching::invisible :spinner :bar"}}>
     <div></div>
     <div></div>
     <div></div>
     <div></div>
     <div></div>
     <div></div>
     <div></div>
     <div></div>
</div>
<div class="goto-button" id="top"><button class="btn btn-small" {{action "goToTop"}} title="Click to return to the top of the table">Go to top</button></div>
<div>
		<ul class="breadcrumb">
  	<li><a href="/">Home</a> <span class="divider">/</span></li>
  	<li>{{objectLink 'pathways' '' this.title this}}<span class="divider">/</span></li>
  	<li class="active">Compounds</li>
			
			<div class="row pull-right"> 
				<div class="span1 small-padding-top small-padding-right">Provenance</div>
  				<div class="span2 btn-group" data-toggle="buttons-radio" style="margin-top:-5px;">
  					<button class="btn">On</button>
  					<button class="btn active">Off</button>
  				</div>
			</div>
		</ul>
		<!-- actual results -->
		<div id="summary-anchor">
		<div id="summary" class="compound_summary collapse in">
			
			<div id="compound-image" class="span2 small-padding-top big-padding-bottom">{{cs_image_src csUri}}</div>
			<div class="row">
				<div class="span8">
    				<h3>{{objectLink 'pathways' '' this.title this}} ({{currentCount}} of {{totalCount}} results loaded)</h3>
  				</div>
  				<button id="toggle-summary" type="button" class="btn btn-link pull-right" data-target="#summary">less</button>	
			</div>
		</div>
		</div>
</div>
<div class="container">
{{#each compound in compounds}}
<p>{{compoundLink compound.URI compound.prefLabel}}</p>
{{/each}}
</div>
