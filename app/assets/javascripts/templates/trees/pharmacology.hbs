{{#linkTo 'trees.index'}}Tree view{{/linkTo}}

<!-- RESULT LISTING -->
	<div style="padding:30px">	
	<table class="mytable">
		<thead id="tablehead" class="headingstyle">
	
			<tr>
				<th class="lead lead-target">Compound</th>
				<th></th>
				<th class="lead lead-assay">Target</th>
				<th></th>
				<th class="lead lead-assay">Assay</th>
				<th></th>
				<th></th>
				<th></th>
				<th class="lead">Activity</th>
				<th></th>
				<th></th>
				<th></th>
				<th></th>
				<th>Structure</th>
				<th></th>

			<tr>
			<tr>
				<th>Structure</th>
				<th>Name</th>
				<th>Names</th>
				<th>Organisms</th>
				<th>Organisms</th>
				<th align="left" width="300px">Description</th>
				<th>Type</th>
			 	<th>Relation</th>
				<th>Value</th>
				<th>Units</th>
				<th>PubMed Article</th>
				<th>Mol Weight</th>
				<th>SMILES</th>
				<th>InChI</th>
				<th>InChIKey</th>
                <th>pChembl</th>
			</tr>
		</thead>
		<tbody>
			{{#each enzymePharma in pharmacology}}
			<tr class="record-deco">
				
				<td class="cell-basictext">{{cs_image_src enzymePharma.csURI}}</td>
				<td class="cell-basictext" style="min-width:300px;">{{compoundSource enzymePharma.cwCompoundURI enzymePharma.prefLabel}}</td>
				<td class="cell-basictext"><button type="button" class="btn btn-link" {{action "navigateTo" enzymePharma.targetURI}}>{{enzymePharma.targetTitle}}</button></td>
				<td class="cell-basictext" style="min-width:120px;">{{enzymePharma.targetOrganism}}</td>
				<td class="cell-basictext">{{enzymePharma.assayOrganism}}</td>
				<td class="cell-longtext" style="min-width:300px;">{{enzymePharma.assayDescription}}</td>
				<td class="lead cell-basictext">{{enzymePharma.activityType}}</td>
				<td class="lead cell-basictext">{{enzymePharma.relation}}</td>
				<td class="lead cell-basictext">{{enzymePharma.standardValue}}</td>
			    <td class="lead cell-basictext">{{enzymePharma.standardUnits}}</td>
			    <td class="cell-basictext">{{linkablePubmedId enzymePharma.pmid}}</td>
			    <td class="cell-basictext">{{enzymePharma.fullMWT}}</td>
			    <td class="cell-longtext">{{enzymePharma.smiles}}</td>
			    <td class="cell-longtext">{{enzymePharma.inchi}}</td>
			    <td class="cell-longtext">{{enzymePharma.inchiKey}}</td>
			    <td class="cell-longtext">{{enzymePharma.pChembl}}</td>

			</tr>
			{{/each}}
		</tbody>
	</table>
	</div>
