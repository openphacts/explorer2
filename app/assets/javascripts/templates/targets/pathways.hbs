<div class="goto-button" id="top">
	<button class="btn btn-default btn-sm" {{action "goToTop"}} title="Click to return to the top of the table">Go to top</button>
</div>
<ul class="breadcrumb hidden-xs">
	<li><a href="/">Home</a></li>
	<li>{{#link-to "targets.index" (query-params uri=URI)}}{{prefLabel}}{{/link-to}}</li>
	<li class="active">Pathways</li>
</ul>
{{partial "compounds/targetPathwaysSummary"}}
<!-- RESULT LISTING -->
{{#if notEmpty}}
<div class="huge-padding-top table-responsive">
	<table class="mytable" style="border-spacing: 0;">
		{{partial "targets/targetPathwaysTableHeader"}}
		{{partial "targets/targetPathwaysTableBody"}}
	</table>
</div>
{{else}}
<h2 class="centre">There are no pathways for {{prefLabel}}</h2>
{{/if}}
