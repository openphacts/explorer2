{{#if notEmpty}}
<table>
	<thead>
		<tr><td>Name</td><td>Disease classes</td></tr>
	</thead>
	<tbody>
		{{#each diseases}}
		<tr>
			<td>{{name}}</td>
			<td>
				{{#if hasDiseaseClasses}}
				<ul class="unstyled">
					{{#each diseaseClass}}
					<li>{{diseaseClassLink this}}</li>
					{{/each}}
				</ul>
				{{/if}}
			</td>
		</tr>
		{{/each}}

	</tbody>
</table>
{{/if}}
